@inherits LayoutComponentBase

<div class="page">
    <MudThemeProvider />
    <MudPopoverProvider />
    <MudDialogProvider />
    <MudSnackbarProvider /> 
    <MudLayout>
        <MudAppBar Fixed="false">
            <MudText Typo="Typo.h5" Edge="Edge.Start">mutual.ie</MudText>
            <MudSpacer/>
            <LogIn></LogIn>
        </MudAppBar>
        <MudMainContent>
            @Body
        </MudMainContent>
    </MudLayout>
</div>

<div id="blazor-error-ui" data-nosnippet>
    An unhandled error has occurred.
    <a href="." class="reload">Reload</a>
    <span class="dismiss">🗙</span>
</div>
